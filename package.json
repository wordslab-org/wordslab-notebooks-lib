{
  "name": "wordslab-notebooks-extension",
  "version": "0.1.0",
  "description": "JupyterLab extension for wordslab-notebooks",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "wordslab-notebooks"
  ],
  "license": "Apache-2.0",
  "author": "Laurent Prud'hon",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension",
    "build:lib": "tsc",
    "build:labextension": "jupyter labextension build .",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/json-schema": "^7.0.0",
    "@types/react": "^18.2.0",
    "typescript": "~5.0.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "wordslab_notebooks_lib/labextension",
    "sharedPackages": {}
  }
}
